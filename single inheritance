class Animal{
    String name;
    Animal(String name){
        this.name=name;
    }
    public void eat(){
        System.out.println(name+"is eating.....");
    }
}
class Dog extends Animal{
    Dog(String name){
        super(name);
    }
    public void bark(){
        System.out.println(name+" is barking...");
    }
}
public class Demo{
    public static void main(String []args){
        Dog d=new Dog("Tom");
        d.eat();
        d.bark();
    }
}
